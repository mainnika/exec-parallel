{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:all"
  ],
  "rules": {
    "indent": [
      true,
      "tabs"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "array-type": [
      true,
      "array"
    ],
    "no-inferrable-types": false,
    "only-arrow-functions": [
      "allow-named-functions"
    ],
    "no-console": [
      false
    ]
  },
  "rulesDirectory": []
}
